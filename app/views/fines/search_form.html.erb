<h1>Search fines</h1>

<%= form_tag do %>
    <%= label_tag(:number, 'Numero:') %>
    <%= text_field_tag(:number) %>
    <br/>

    <%= label_tag(:plate, 'Targa:') %>
    <%= text_field_tag(:plate) %>
    <br/>

    <%= label_tag(:taken_at, 'Data:') %>
    <%= date_field_tag(:taken_at) %>
    <br/>

    <%= submit_tag('Cerca') %>
<% end %>
